{
    "name": "atstudio-tech/breadcrumbs",
    "description": "Yet another breadcrumbs package for Laravel: simple and easy to use.",
    "type": "library",
    "keywords": [
        "laravel",
        "breadcrumbs",
        "crumbs",
        "navigation"
    ],
    "readme": "./README.md",
    "license": "MIT",
    "require": {
        "php": "8.1",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "orchestra/testbench": "^7.1",
        "phpunit/phpunit": "^9.5"
    },
    "replace": {
        "atorscho/laravel-breadcrumbs": "2.4.*"
    },
    "autoload": {
        "psr-4": {
            "ATStudio\\Breadcrumbs\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ATStudio\\Breadcrumbs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ATStudio\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
